<%= simple_form_for(vehicle_checkout) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :checkout_date %>
    <%= f.input :return_date %>
    <%= f.input :crew_id %>
    <%= f.input :vehicle_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
